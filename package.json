{
  "name": "charlike",
  "description": "Small & fast project scaffolder with sane defaults. Supports hundreds of template engines through the @JSTransformers API or if you want custom `render` function passed through options",
  "license": "Apache-2.0",
  "licenseStart": "2016",
  "scripts": {
    "cli": "node cli.js",
    "start": "tunnckocore-scripts",
    "extends": "@tunnckocore/config/scripts"
  },
  "engines": {
    "node": "^8.11.0 || >=10.13.0"
  },
  "dependencies": {
    "@tunnckocore/package-json": "^0.3.1",
    "camelcase": "^5.0.0",
    "dateformat": "^3.0.3",
    "esm": "^3.0.84",
    "git-config-path": "^1.0.1",
    "git-user-email": "^0.2.2",
    "git-user-name": "^2.0.0",
    "git-username": "^1.0.0",
    "jstransformer": "^1.0.0",
    "jstransformer-j140": "^1.0.0",
    "jstransformer-jstransformer": "^1.1.6",
    "mri": "^1.1.1",
    "parse-git-config": "^2.0.3",
    "stream-copy-dir": "^1.0.0"
  },
  "devDependencies": {
    "@tunnckocore/config": "^0.5.0",
    "@tunnckocore/scripts": "^1.0.1",
    "asia": "^0.19.7"
  },
  "files": [
    "src",
    "cli.js",
    "index.js",
    "templates"
  ],
  "bin": "cli.js",
  "main": "index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "version": "0.0.0-semantically-released",
  "repository": "tunnckoCoreLabs/charlike",
  "homepage": "https://github.com/tunnckoCoreLabs/charlike",
  "author": "Charlike Mike Reagent (https://tunnckocore.com)",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "asia",
        "docks",
        "gitcommit",
        "new-release",
        "jstransformer"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "new-release",
      "semantic-release"
    ]
  }
}
