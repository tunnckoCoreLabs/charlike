{
  "name": "charlike",
  "description": "Small, fast, simple and streaming project scaffolder for myself, but not only. Supports hundreds of template engines through the @JSTransformers API or if you want custom `render` function passed through options",
  "nspId": "e6f9f165-cc4d-41be-ad83-aa54e30caaab",
  "license": "Apache-2.0",
  "licenseStart": 2016,
  "scripts": {
    "lint": "hela lint",
    "test": "hela test",
    "commit": "hela commit --no-docs",
    "prepublishOnly": "hela protect"
  },
  "engines": {
    "node": ">=8.6",
    "npm": ">=5.2",
    "yarn": ">=1.3"
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "dateformat": "^3.0.3",
    "jstransformer": "^1.0.0",
    "jstransformer-j140": "^1.0.0",
    "jstransformer-jstransformer": "^1.1.6",
    "resolve": "^1.5.0",
    "stream-copy-dir": "^1.0.0"
  },
  "devDependencies": {
    "hela": "^2.0.9",
    "hela-config-tunnckocore": "^3.0.16",
    "mukla": "^0.4.9"
  },
  "files": [
    "dist",
    "templates"
  ],
  "eslintConfig": {
    "extends": "tunnckocore"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "hela": {
    "extends": "tunnckocore"
  },
  "rollup": {
    "input": "src/index.js",
    "output": [
      "cjs",
      "es"
    ]
  },
  "nyc": {
    "statements": 82,
    "functions": 81,
    "branches": 62,
    "lines": 81,
    "exclude": [
      "test"
    ],
    "extension": [
      ".js",
      ".jsx",
      ".mjs"
    ]
  },
  "version": "0.0.0-semantically-released",
  "repository": "tunnckoCore/charlike",
  "homepage": "https://github.com/tunnckoCore/charlike",
  "author": "Charlike Mike Reagent (http://tunnckocore.com)",
  "cdn": "./dist/index.umd.js",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3,
      "footer": ""
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "gitcommit",
        "hela",
        "hela-config-tunnckocore",
        "jstransformer"
      ],
      "highlight": "charlike-cli"
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "execa",
      "hela",
      "new-release"
    ]
  }
}
